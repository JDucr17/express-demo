{
  "name": "express-demo",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "node esbuild.config.mjs",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node dist/scripts/migrate.js",
    "db:seed:prod": "node dist/scripts/seed.js",
    "db:seed:local": "DATABASE_URL='postgresql://postgres:postgres@localhost:5432/sac_mate' tsx src/scripts/seed.js",
    "db:seed:remote": "tsx src/scripts/seed.js",
    "dev": "tsx src/server.ts",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f backend",
    "db:migrate:kit": "drizzle-kit migrate",
    "start": "node dist/server.js",
    "docker:rebuild": "docker compose build --no-cache",
    "docker:ps": "docker compose ps",
    "docker:exec": "docker exec -it backend-sac-mate sh",
    "docker:restart": "npm run docker:down && npm run docker:up",
    "docker:reset": "docker compose down -v && docker compose up -d --build",
    "docker:migrate": "docker compose exec backend node dist/scripts/migrate.ts",
    "docker:seed": "docker compose exec backend node dist/scripts/seed.ts",
    "docker:db": "docker compose exec postgres psql -U postgres -d sac_mate",
    "dev:db": "docker compose up -d postgres",
    "dev:db:stop": "docker compose stop postgres",
    "dev:local": "DATABASE_URL='postgresql://postgres:postgres@localhost:5432/sac_mate' npm run dev",
    "db:push": "DATABASE_URL='postgresql://postgres:postgres@localhost:5432/sac_mate' drizzle-kit push",
    "db:studio": "DATABASE_URL='postgresql://postgres:postgres@localhost:5432/sac_mate' drizzle-kit studio",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "cross-env NODE_ENV=test vitest --run --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:coverage": "cross-env NODE_ENV=test vitest --run --coverage",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "better-auth": "^1.3.34",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "pg": "^8.16.3",
    "pino": "^9.9.4",
    "pino-http": "^10.5.0",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "patch-package": "^8.0.1",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=20"
  }
}
